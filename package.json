{
  "name": "react-dccs-starter-kit",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^25.1.1",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^1.0.1",
    "html-webpack-externals-plugin": "^3.8.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "raw-loader": "^4.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "tslint": "^6.0.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^4.2.0",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.2",
    "webpack-merge": "^4.2.2"
  },
  "scripts": {
    "start": "webpack-dev-server --inline --hot --content-base --mode development --config webpack.dev.config",
    "api": "node tools/apiServer.js",
    "build": "webpack --mode production --config webpack.config",
    "tslint": "tslint \"src/**/*.{ts,tsx}\" -c tslint.json",
    "prettier": "prettier --write \"src/**/*.tsx\" \"tests/**/*.tsx\"",
    "prettiercss": "prettier --parser css --write \"src/**/*.scss\"",
    "precommit": "npm run prettier && npm run prettiercss && npm run tslint && npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.3",
    "cross-env": "^5.2.0",
    "json-server": "^0.15.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.7.5"
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false,
    "parser": "typescript",
    "printWidth": 120,
    "bracketSpacing": true,
    "arrowParens": "always"
  }
}
